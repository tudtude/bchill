{
    "compilerOptions": {
        "types": ["bun-types"],
        "module": "CommonJS",
        "target": "ES2015",
        "lib": ["ESNext"],
        "declaration": true,
        "strict": true,
        "moduleResolution": "node10",
        "skipLibCheck": true,
        "outDir": "dist",
        "noEmit": true
    },
    "include": ["src"],
    "exclude": ["node_modules/*"]
}
